{
  "name": "webservices-budget",
  "version": "0.0.1",
  "scripts": {
    "start": "npx env-cmd nodemon",
    "test": "npx env-cmd -f .env.test jest",
    "test:coverage": "npx env-cmd -f .env.test jest --coverage"
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "argon2": "^0.28.2",
    "config": "^3.3.6",
    "env-cmd": "^10.1.0",
    "jest": "^27.3.1",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "mysql2": "^2.3.0",
    "node-emoji": "^1.11.0",
    "serialize-error": "^8.1.0",
    "supertest": "^6.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
